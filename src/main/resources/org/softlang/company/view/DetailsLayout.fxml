<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<FlowPane maxWidth="1.7976931348623157E308" orientation="VERTICAL" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.softlang.company.view.DetailsController">
	<children>
		<Label fx:id="title" text="Company" underline="true">
			<font>
				<Font size="20.0" />
			</font>
		</Label>
		<GridPane alignment="CENTER_LEFT">
			<columnConstraints>
				<ColumnConstraints fillWidth="false" hgrow="NEVER" maxWidth="60.0" minWidth="10.0" prefWidth="60.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints fillHeight="false" minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
				<RowConstraints fillHeight="false" minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
				<RowConstraints fillHeight="false" minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
			</rowConstraints>
			<children>
				<Label fx:id="nameLabel" text="Name:" />
				<TextField fx:id="nameContent" prefHeight="31.0" prefWidth="128.0" GridPane.columnIndex="1" />
				<Label fx:id="addressLabel" text="Address:" GridPane.rowIndex="1" />
				<TextField fx:id="addressContent" prefWidth="125.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
				<Label fx:id="salaryLabel" text="Salary:" GridPane.rowIndex="2" />
				<TextField fx:id="salaryContent" prefWidth="125.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
			</children>
		</GridPane>
	</children>
</FlowPane>
